{
  "name": "acp",
  "version": "1.0.0",
  "description": "Admin Panel for the Plugin",
  "main": "index.js",
  "scripts": {
    "build": "browserify index.js -t babelify --outfile ../../public/js/acp.js",
    "watch-js": "watchify index.js -o ../../public/js/acp.js --verbose",
    "watch-less": "gulp",
    "watch": "npm run watch-less & npm run watch-js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "acp",
    "client",
    "es6"
  ],
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "blacklist": [
            "regenerator"
          ]
        }
      ],
      "envify",
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "app": "global:app",
    "jquery": "global:$",
    "socket": "global:socket",
    "define": "global:define",
    "bootstrap": {
      "exports": "global:bootstrap",
      "depends": [
        "jquery"
      ]
    },
    "bootbox": {
      "exports": "global:bootbox",
      "depends": [
        "jquery",
        "bootstrap"
      ]
    }
  },
  "author": "Nicolas Siver",
  "license": "MIT",
  "dependencies": {
    "classnames": "^1.2.2",
    "lodash": "^3.9.3",
    "react": "^0.13.1",
    "reflux": "^0.2.8"
  },
  "devDependencies": {
    "babelify": "^6.1.2",
    "browserify": "^9.0.8",
    "browserify-shim": "^3.8.5",
    "envify": "^3.4.0",
    "gulp": "^3.9.0",
    "gulp-less": "^3.0.3",
    "gulp-plumber": "^1.0.1",
    "gulp-watch-less": "treshugart/gulp-watch-less",
    "less": "^2.5.1",
    "uglify-js": "^2.4.20"
  }
}
